    <style>
    /*Styles for the share buttons*/
                .fa-button {
      background-color: white;
      border-radius: 3.6rem;
      cursor: pointer;
      display: inline-block;
      font-size: 2.0rem;
      height: 3.6rem;
      line-height: 3.6rem;
      margin: 0 5px;
      position: relative;
      text-align: center;
      -webkit-user-select: none;
         -moz-user-select: none;
          -ms-user-select: none;
              user-select: none;
      width: 3.6rem;
    }

    /* Circle */
    .fa-button span {
      border-radius: 0;
      display: block;
      height: 0;
      left: 50%;
      margin: 0;
      position: absolute;
      top: 50%;
      -webkit-transition: all 0.3s;
         -moz-transition: all 0.3s;
           -o-transition: all 0.3s;
              transition: all 0.3s;
      width: 0;
    }
    .fa-button:hover span {
      width: 3.6rem;
      height: 3.6rem;
      border-radius: 3.6rem;
      margin: -1.8rem;
    }
    .twitter span {
      background-color: #4099ff;
    }
    .facebook span {
      background-color: #3B5998;
    }
    .google-plus span {
      background-color: #db5a3c;
    }

    /* fas */
    .fa-button i {
      background: none;
      color: white;
      height: 3.6rem;
      left: 0;
      line-height: 3.6rem;
      position: absolute;
      top: 0;
      -webkit-transition: all 0.3s;
         -moz-transition: all 0.3s;
           -o-transition: all 0.3s;
              transition: all 0.3s;
      width: 3.6rem;
      z-index: 10;
    }
    .fa-button .fa-twitter {
      color: #4099ff;
    }
    .fa-button .fa-facebook {
      color: #3B5998;
    }
    .fa-button .fa-google-plus {
      color: #db5a3c;
    }
    .fa-button:hover .fa-twitter,
    .fa-button:hover .fa-facebook,
    .fa-button:hover .fa-google-plus {
      color: white;
    }
    .isStuck{
    left:0px;
    width:100%;
}
    </style>
